{
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "description": "Torrentd is a torrent RSS feed generator.",
        "title": "Torrentd API",
        "contact": {},
        "version": "1.0.0"
    },
    "host": "localhost:5000",
    "basePath": "/",
    "paths": {
        "/status": {
            "get": {
                "description": "get status of the server",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "status"
                ],
                "summary": "Status of the server",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/server.statusResponse"
                        }
                    }
                }
            }
        },
        "/torznab/caps/{indexes}": {
            "get": {
                "description": "Get the index(es) capabilities in torznab format",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "text/xml"
                ],
                "tags": [
                    "torznab"
                ],
                "summary": "Torznab capabilities",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Index name(s) to get the capabilities for",
                        "name": "indexes",
                        "in": "path"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/torznab.Capabilities"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/torznab.err"
                        }
                    }
                }
            }
        },
        "/torznab/{indexes}": {
            "get": {
                "description": "Query indexes in torznab format",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "text/xml",
                    "application/json"
                ],
                "tags": [
                    "torznab"
                ],
                "summary": "Torznab handler",
                "parameters": [
                    {
                        "type": "string",
                        "default": "all",
                        "description": "Index name(s) to search through",
                        "name": "indexes",
                        "in": "path"
                    },
                    {
                        "type": "string",
                        "description": "Type of search. Can be caps, search, tvsearch, tv-search, movie, movie-search, moviesearch. Defaults to caps, returning the capabilities.",
                        "name": "t",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Search query",
                        "name": "q",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Category",
                        "name": "cat",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "The output format to use",
                        "name": "format",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "IMDB ID",
                        "name": "imdbid",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "TMDB ID",
                        "name": "tmdbid",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "TVDB ID",
                        "name": "rid",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Season number",
                        "name": "season",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Episode number",
                        "name": "ep",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Limit the number of results, defaults to 20",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Offset the results",
                        "name": "offset",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Minimum age of the torrent",
                        "name": "minage",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Maximum age of the torrent",
                        "name": "maxage",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Minimum size of the torrent",
                        "name": "minsize",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Maximum size of the torrent",
                        "name": "maxsize",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "API key",
                        "name": "apikey",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/torznab.ResultFeed"
                        }
                    },
                    "404": {
                        "description": "404 page not found",
                        "schema": {
                            "type": "type"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "categories.Categories": {
            "type": "object",
            "additionalProperties": {
                "$ref": "#/definitions/categories.Category"
            }
        },
        "categories.Category": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "models.IndexStatus": {
            "type": "object",
            "properties": {
                "errors": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "index": {
                    "type": "string"
                },
                "is_aggregate": {
                    "type": "boolean"
                },
                "size": {
                    "type": "integer"
                }
            }
        },
        "models.LatestResult": {
            "type": "object",
            "properties": {
                "desc": {
                    "type": "string"
                },
                "link": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "site": {
                    "type": "string"
                }
            }
        },
        "search.Capability": {
            "type": "object",
            "properties": {
                "available": {
                    "type": "boolean"
                },
                "key": {
                    "type": "string"
                },
                "supportedParams": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "server.statusResponse": {
            "type": "object",
            "properties": {
                "indexes": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.IndexStatus"
                    }
                },
                "latest": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.LatestResult"
                    }
                }
            }
        },
        "torznab.Capabilities": {
            "type": "object",
            "properties": {
                "categories": {
                    "$ref": "#/definitions/categories.Categories"
                },
                "searchModes": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/search.Capability"
                    }
                }
            }
        },
        "torznab.Info": {
            "type": "object",
            "properties": {
                "category": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "language": {
                    "type": "string"
                },
                "link": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "torznab.ResultFeed": {
            "type": "object",
            "properties": {
                "info": {
                    "$ref": "#/definitions/torznab.Info"
                },
                "items": {
                    "type": "array",
                    "items": {}
                }
            }
        },
        "torznab.err": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "description": {
                    "type": "string"
                }
            }
        }
    }
}